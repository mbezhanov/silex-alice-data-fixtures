{
    "name": "mbezhanov/silex-alice-data-fixtures",
    "description": "A library providing easy integration of Alice and Doctrine Data Fixtures into Silex projects",
    "keywords": ["silex", "doctrine", "alice", "fixtures"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marin Bezhanov",
            "email": "marin.bezhanov@gmail.com",
            "homepage": "http://marinbezhanov.com"
        }
    ],
    "autoload": {
        "psr-4": { "Bezhanov\\Silex\\AliceDataFixtures\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "Bezhanov\\Silex\\AliceDataFixtures\\Tests\\": "tests" }
    },
    "require": {
        "php": "^7.0",
        "silex/silex": "^2.1",
        "nelmio/alice": "^3.0@RC",
        "doctrine/data-fixtures": "^1.2",
        "symfony/console": "^3.3",
        "dflydev/doctrine-orm-service-provider": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    }
}
